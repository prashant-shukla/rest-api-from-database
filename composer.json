{
    "name": "prashant-shukla/rest-api-from-database",
    "type": "library",
    "description": "Auto generate REST API from database",
    "keywords": [
        "prashant-shukla",
        "rest-api-from-database"
    ],
    "homepage": "https://github.com/prashant-shukla/rest-api-from-database",
    "license": "MIT",
    "authors": [
        {
            "name": "Prashant Shukla",
            "email": "prashant.jod@gmail.com",
            "homepage": "https://ishalabs.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "~7.2"
    },
    "require-dev": {
        "phpunit/phpunit" : ">=8.0",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "prashant-shukla\\rest-api-from-database\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "prashant-shukla\\rest-api-from-database\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
